#include <iostream>
#include <string>
using namespace std; 

struct PC
{
    string pcPurpose;        //Назначение ПК: Работа/Обучение/Развлечение/Хобби и творчество
    string pcType;          //Тип ПК: Десктоп/Ноутбук/Смартфон
    int cores;              //Кол-во ядер процессора
    int coreFrequency;      //Частота процессора (МГц)

    int coolerPower;        //Мощность кулера (Вт)

    string motherChipset;   //Набор встроенных микросхем

    string ramGeneration;   //Поколение ОЗУ
    int ramMemory;
    int ramFrequency;       //Частота ОЗУ (ГГц)
    string romType;
    int romMemory;          //Объем постоянной памяти (Гб)

    int videoFrequency;     //Частота работы видеочипа
    int videoMemoryVolume;  //Объем видеопамяти
    string videoMemoryType; //Тип видеопамяти
    string videoInterface;  //Интерфейс видеокарты

    int powerUnitEnergy;    //Мощность блока питания

    string OS;

    int price;              //Цена
};

