    int step = 0;
    string purpose;
    string typepc;
    int wallet;
    int len = sizeof(arrPC)/sizeof(arrPC[0]);
    
    while (step >= 0) {
        std::cout << "Вас приветствует Автономный Обозреватель Персональных Компьютеров. Я помогу вам выбрать ПК вашей мечты, или по размерам вашего кошелька!" << endl
            << "Пожалуйста, введите вариант назначение вашего ПК: (1)Работа, (2)Обучение, (3)Развлечение, (4)Хобби и Творчество." << endl;
        int input1;
        cin >> input1;
        switch (input1)
        {
        case 1:
            purpose = "Work";
            break;
        case 2:
            purpose = "Education";
            break;
        case 3:
            purpose = "Gaming";
            break;
        case 4:
            purpose = "Development";
            break;
        default:
            std::cout << "Ошибка ввода, попробуйте еще раз." << endl;
            std::cin >> input1;
            continue;
        }

        std::cout << "Пожалуйста, введите тип желаемого ПК: (1)Десктоп, (2)Ноутбук, (3)Планшет." << endl;
        std::cin >> input1;
        switch (input1)
        {
        case 1:
            typepc = "Desktop";
            break;
        case 2:
            typepc = "Laptop";
            break;
        case 3:
            typepc = "Tablet";
            break;
        default:
            std::cout << "Ошибка ввода, попробуйте еще раз." << endl;
            std::cin >> input1;
            continue;
        }

        std::cout << "Пожалуйста, введите желаемую максимальную цену." << endl;
        std::cin >> wallet;

        for (int i = 0; i < len; i++) {
            if (purpose==arrPC[i].pcPurpose&&typepc==arrPC[i].pcType&&wallet<=arrPC[i].price) {
                cout << "Назначение ПК: " << arrPC[i].pcPurpose << endl
                    << "Тип ПК: " << arrPC[i].pcType << endl
                    << "Количество ядер: " << arrPC[i].cores << endl
                    << "Частота процессора (МГц): " << arrPC[i].coreFrequency << endl
                    << "Мощность кулера (Вт): " << arrPC[i].coolerPower << endl
                    << "Чипсет материнской платы: " << arrPC[i].motherChipset << endl
                    << "Поколение ОЗУ: " << arrPC[i].ramGeneration << endl
                    << "Оперативная память: " << arrPC[i].ramMemory << endl
                    << "Частота ОЗУ (ГГц)" << arrPC[i].coreFrequency << endl
                    << "Тип постоянной памяти: " << arrPC[i].romType << endl
                    << "Объем постоянной памяти(Гб): " << arrPC[i].romMemory << endl
                    << "Частота работы видеочипа: " << arrPC[i].videoFrequency << endl
                    << "Объем видеопамяти: " << arrPC[i].videoMemoryVolume << endl
                    << "Тип видеопамяти: " << arrPC[i].videoMemoryType << endl
                    << "Интерфейс видеокарты: " << arrPC[i].videoInterface << endl
                    << "Мощность блока питания: " << arrPC[i].powerUnitEnergy << endl
                    << "Операционная система: " << arrPC[i].OS << endl
                    << "Цена: " << arrPC[i].price << endl;
            }
            else
            {
                continue;
            }
        }
        break;
        
    }
    
}
